    private bool scriptsLoaded = false;

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender && !scriptsLoaded)
        {
            var baseUri = NavigationManager.BaseUri;

            // Load local scripts
            await ScriptLoader.LoadScriptAsync($"{baseUri}assets/libs/jsvectormap/jsvectormap.min.js");
            await ScriptLoader.LoadScriptAsync($"{baseUri}assets/libs/jsvectormap/maps/world-merc.js");
            await ScriptLoader.LoadScriptAsync($"{baseUri}assets/js/widgets.js");

            // Execute the map initialization function after scripts are loaded
            await JSRuntime.InvokeVoidAsync("initializeMap");

            scriptsLoaded = true;
            StateHasChanged();
        }
    }
}

