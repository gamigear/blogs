@page "/dashboards/socialmedia"
@using ApexCharts


<!-- Start::page-header -->
<div class="page-header-breadcrumb mb-3">
    <div class="d-flex align-center justify-content-between flex-wrap">
        <h1 class="page-title fw-medium fs-18 mb-0">Social Media</h1>
        <ol class="breadcrumb mb-0">
            <li class="breadcrumb-item"><a href="javascript:void(0);">Dashboards</a></li>
            <li class="breadcrumb-item active" aria-current="page">Social Media</li>
        </ol>
    </div>
</div>
<!-- End::page-header -->

<!-- Start:: row-1 -->
<div class="row">
    <div class="col-xxl-9">
        <div class="row">
            @foreach(var card in statCards)
            {
                <div class="col-xxl-3 col-md-6">
                    <div class="card custom-card dashboard-main-card">
                        <div class="card-body">
                            <div class="d-flex align-items-start gap-2 flex-wrap">
                                <div class="lh-1">
                                    <span class="avatar avatar-md avatar-rounded @card.BgClass @card.SvgClass" 
                                        style="display:flex; align-items:center; justify-content:center;">
                                        @((MarkupString)(card.IconSvg?? string.Empty))
                                    </span>
                                </div>
                                <div class="flex-fill">
                                    <div class="d-flex align-items-end justify-content-between">
                                        <span class="d-block mb-1">@card.Title</span>
                                        <div class="dropdown">
                                            <a aria-label="anchor" href="javascript:void(0);" data-bs-toggle="dropdown"
                                            aria-expanded="false" class="btn btn-icon btn-sm rounded-circle btn-light">
                                                <i class="ti ti-dots text-muted fs-5"></i>
                                            </a>
                                            <ul class="dropdown-menu" role="menu">
                                                <li><a class="dropdown-item" href="javascript:void(0);">Today</a></li>
                                                <li><a class="dropdown-item" href="javascript:void(0);">This Week</a></li>
                                                <li><a class="dropdown-item" href="javascript:void(0);">This Month</a></li>
                                                <li><a class="dropdown-item" href="javascript:void(0);">This Year</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="d-flex align-items-end justify-content-between">
                                        <h4 class="fw-semibold mb-0">@card.Value</h4>
                                        <div class="@(card.IsPositiveTrend ? "text-success" : "text-danger") fs-13">
                                            <i class="ti @(card.IsPositiveTrend ? "ti-arrow-up" : "ti-arrow-down")"></i>
                                            @card.PercentChange.ToString("0.##")%
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
            <div class="col-xl-8">
                <div class="card custom-card">
                    <div class="card-header">
                        <div class="card-title">
                            Profile Visits
                        </div>
                    </div>
                    <div class="card-body">
                        <div id="profile-analysis">
                        <ApexChart TItem="ProfileVisits" Height="320" Options="ProfileVisitsChartOptions">
                            <ApexPointSeries TItem="ProfileVisits" Items="profileVisits" SeriesType="@SeriesType.Line" Name="Facebook"
                                XValue="@(e => e.Month)" YAggregate="@(e => e.Sum(e => e.Facebook))" />
                            <ApexPointSeries TItem="ProfileVisits" Items="profileVisits" SeriesType="@SeriesType.Line" Name="Instagram"
                                XValue="@(e => e.Month)" YAggregate="@(e => e.Sum(e => e.Instagram))" />
                            <ApexPointSeries TItem="ProfileVisits" Items="profileVisits" SeriesType="@SeriesType.Line" Name="Twitter"
                                XValue="@(e => e.Month)" YAggregate="@(e => e.Sum(e => e.Twitter))" />
                        </ApexChart>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-4">
                <div class="card custom-card">
                    <div class="card-header">
                        <div class="card-title">
                            Audience By Gender
                        </div>
                    </div>
                    <div class="card-body">
                        <div id="audience-by-gender">
                        <ApexChart TItem="RadarMulti"  Height="280"  Options=RadarMultiChartOptions>
                            <ApexPointSeries TItem="RadarMulti" Items="RadarMultis" Name="Male"
                                    SeriesType="SeriesType.Radar"
                                    XValue="@(e => e.Country)"
                                    YAggregate="@(e => e.Sum(e => e.Male))" />
                            <ApexPointSeries TItem="RadarMulti" Items="RadarMultis" Name="Female"
                                    SeriesType="SeriesType.Radar"
                                    XValue="@(e => e.Country)"
                                    YAggregate="@(e => e.Sum(e => e.Female))" />
                    </ApexChart>
                        </div>
                        <div class="row mt-0">
                            <div class="col-6 border-end border-inline-end-dashed text-center">
                                <p class="text-muted mb-1 fs-12">Male</p>
                                <h6 class="text-primary mb-0">12.34K</h6>
                            </div>
                            <div class="col-6 text-center">
                                <p class="text-muted mb-1 fs-12">Female</p>
                                <h6 class="text-secondary mb-0">10.19K</h6>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-6">
                <div class="card custom-card overflow-hidden">
                    <div class="card-header">
                        <div class="card-title">
                            Top Audience Countries
                        </div>
                    </div>
                    <div class="card-body p-0">
                        <div class="table-responsive">
                             <table class="table text-nowrap">
                                <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Country</th>
                                        <th>Engagement</th>
                                        <th>Followers</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 0; i < countries.Count; i++)
                                    {
                                        var country = countries[i];
                                        bool isLast = (i == countries.Count - 1);
                                        string tdClass = isLast ? "border-bottom-0" : "";

                                        <tr>
                                            <td class="@tdClass">@country.Rank</td>
                                            <td class="@tdClass">
                                                <div class="d-flex align-items-center gap-2">
                                                    <div class="lh-1">
                                                        <span class="avatar avatar-sm">
                                                            <img src="@country.FlagUrl" alt="@country.CountryName" />
                                                        </span>
                                                    </div>
                                                    <div class="fw-semibold text-truncate">@country.CountryName</div>
                                                </div>
                                            </td>
                                            <td class="@tdClass">
                                                <span class="d-block">
                                                    @String.Format("{0:n0}", country.Likes)
                                                    <i class="ti ti-thumb-up text-muted fs-18 ms-1"></i>
                                                </span>
                                            </td>
                                            <td class="@tdClass">
                                                <div class="d-block">
                                                    @String.Format("{0:n0}", country.Views)
                                                    <span class="fs-12 @(country.IsTrendingUp ? "text-success" : "text-danger") ms-1 d-inline-flex">
                                                        <i class="ti @(country.IsTrendingUp ? "ti-trending-up" : "ti-trending-down") me-1 align-middle"></i>
                                                        @country.TrendPercent.ToString("0.##")%
                                                    </span>
                                                </div>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-6">
                <div class="card custom-card overflow-hidden">
                    <div class="card-header">
                        <div class="card-title">
                            Post Insights
                        </div>
                    </div>
                    <div class="card-body p-0">
                        <div class="table-responsive">
                            <table class="table text-nowrap">
                                <thead>
                                    <tr>
                                        <th scope="col">Post Name</th>
                                        <th scope="col">Posted</th>
                                        <th scope="col">Platform</th>
                                        <th scope="col">Views</th>
                                        <th scope="col" class="text-center">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 0; i < posts.Count; i++)
                                    {
                                        var post = posts[i];
                                        bool isLast = (i == posts.Count - 1);
                                        string tdClass = isLast ? "border-bottom-0" : "";

                                        <tr>
                                            <td class="@tdClass">
                                                <div class="d-flex align-items-center gap-2">
                                                    <div class="lh-1">
                                                        <span class="avatar avatar-sm">
                                                            <img src="@post.ImageUrl" alt="@post.Title" />
                                                        </span>
                                                    </div>
                                                    <span class="fw-medium">@post.Title</span>
                                                </div>
                                            </td>
                                            <td class="@tdClass">@post.Date</td>
                                            <td class="@tdClass">
                                                <span class="badge bg-@post.BadgeClass-transparent">@post.Platform</span>
                                            </td>
                                            <td class="@tdClass">@post.Reach</td>
                                            <td class="text-end @tdClass">
                                                <div class="dropdown">
                                                    <a aria-label="anchor" href="javascript:void(0);" data-bs-toggle="dropdown" aria-expanded="false"
                                                    class="btn btn-icon btn-sm btn-primary-light">
                                                        <i class="ti ti-dots-vertical"></i>
                                                    </a>
                                                    <ul class="dropdown-menu" role="menu">
                                                        <li>
                                                            <a class="dropdown-item d-inline-flex align-items-center" href="javascript:void(0);">
                                                                <i class="ti ti-edit me-2"></i>Edit
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a class="dropdown-item d-inline-flex align-items-center" href="javascript:void(0);" @onclick="@(() => DeletePost(post))">
                                                                <i class="ti ti-trash me-2"></i>Delete
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                            @if(posts.Count < 1){
                                <div class="text-center">
                                    <p class="fw-medium m-3">No Data Availble</p>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-12">
                <div class="card custom-card">
                    <div class="card-header justify-content-between">
                        <div class="card-title">
                            Social Media Performance Overview
                        </div>
                        <div class="d-flex flex-wrap gap-2">
                            <div>
                                <input class="form-control form-control-sm" type="text" placeholder="Search Here"
                                    aria-label=".form-control-sm example">
                            </div>
                            <div class="dropdown">
                                <a href="javascript:void(0);"
                                    class="btn btn-primary btn-sm btn-wave waves-effect waves-light"
                                    data-bs-toggle="dropdown" aria-expanded="false"> Sort By<i
                                        class="ri-arrow-down-s-line align-middle ms-1 d-inline-block"></i>
                                </a>
                                <ul class="dropdown-menu" role="menu">
                                    <li><a class="dropdown-item" href="javascript:void(0);">New</a></li>
                                    <li><a class="dropdown-item" href="javascript:void(0);">Popular</a></li>
                                    <li><a class="dropdown-item" href="javascript:void(0);">Relevant</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="card-body p-0">
                        <div class="table-responsive">
                            <table class="table text-nowrap">
                                <thead>
                                    <tr>
                                        <th><input type="checkbox" class="form-check-input" @onchange="ToggleSelectAll" checked="@selectAll" /></th>
                                        <th>Platform</th>
                                        <th>Posts</th>
                                        <th>Likes</th>
                                        <th>Shares</th>
                                        <th>Comments</th>
                                        <th>Impressions</th>
                                        <th>Followers</th>
                                        <th>CTR (%)</th>
                                        <th>Actions</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in socialData)
                                    {
                                        <tr>
                                            <td>
                                                <input class="form-check-input" type="checkbox" @bind="item.IsChecked" />
                                            </td>
                                            <td>
                                                <div class="d-flex align-items-center gap-2">
                                                    <div class="lh-1">
                                                        <span class="avatar avatar-sm bg-@item.avatarBgClass-transparent">
                                                            <i class="@item.IconClass fs-5"></i>
                                                        </span>
                                                    </div>
                                                    <div>@item.Platform</div>
                                                </div>
                                            </td>
                                            <td>@item.Posts</td>
                                            <td>@item.Likes</td>
                                            <td>@item.Shares</td>
                                            <td>@item.Comments</td>
                                            <td>@item.Impressions</td>
                                            <td>@item.Followers</td>
                                            <td>
                                                <span class="badge bg-@item.ctrBadgeBg-transparent">@item.Ctr</span>
                                            </td>
                                            <td>
                                                <div class="dropdown">
                                                    <a aria-label="anchor" href="javascript:void(0);" data-bs-toggle="dropdown" aria-expanded="false" class="btn btn-icon btn-sm btn-light">
                                                        <i class="ti ti-dots-vertical"></i>
                                                    </a>
                                                    <ul class="dropdown-menu" role="menu">
                                                        <li><a class="dropdown-item"><i class="ti ti-eye me-2 d-inline-block"></i>View</a></li>
                                                        <li><a class="dropdown-item"><i class="ti ti-edit me-2 d-inline-block"></i>Edit</a></li>
                                                        <li><a class="dropdown-item" @onclick="() => DeleteItem(item)"><i class="ti ti-trash me-2 d-inline-block"></i>Delete</a></li>
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="card-footer border-top-0">
                        <div class="d-flex align-items-center">
                            <div> Showing 6 Entries <i class="bi bi-arrow-right ms-2 fw-semibold"></i> </div>
                            <div class="ms-auto">
                                <nav aria-label="Page navigation" class="pagination-style-2">
                                    <ul class="pagination mb-0 flex-wrap">
                                        <li class="page-item disabled">
                                            <a class="page-link" href="javascript:void(0);">
                                                Prev
                                            </a>
                                        </li>
                                        <li class="page-item"><a class="page-link" href="javascript:void(0);">1</a></li>
                                        <li class="page-item active"><a class="page-link"
                                                href="javascript:void(0);">2</a></li>
                                        <li class="page-item">
                                            <a class="page-link" href="javascript:void(0);">
                                                <i class="bi bi-three-dots"></i>
                                            </a>
                                        </li>
                                        <li class="page-item"><a class="page-link" href="javascript:void(0);">17</a>
                                        </li>
                                        <li class="page-item">
                                            <a class="page-link text-primary" href="javascript:void(0);">
                                                next
                                            </a>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xxl-3">
        <div class="row">
            <div class="col-xl-12">
                <div class="card custom-card">
                    <div class="card-header">
                        <div class="card-title">
                            Recent Activity
                        </div>
                    </div>
                    <div class="card-body">
                        <ul class="list-unstyled social-recent-activity-list">                         
                            @foreach (var activity in recentActivities)
                            {
                                <li>
                                    <div class="d-flex align-items-start gap-3">
                                        <div class="lh-1">
                                            <span class="avatar avatar-md avatar-rounded bg-@activity.AvatarBgClass-transparent">
                                                <i class="ri-@activity.IconClass fs-5"></i>
                                            </span>
                                        </div>
                                        <div class="flex-fill">
                                            <div class="d-flex align-items-start gap-2 justify-content-between mb-1">
                                                <span class="fw-semibold d-block">@activity.Platform</span>
                                                <span class="badge bg-@activity.BadgeClass-transparent">@activity.Time</span>
                                            </div>
                                            <div class="fs-13 text-muted social-activity">@activity.ActivityText</div>
                                        </div>
                                    </div>
                                </li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-xl-12">
                <div class="card custom-card">
                    <div class="card-header">
                        <div class="card-title">
                            Audience Age Metrics
                        </div>
                    </div>
                    <div class="card-body py-0">
                        <div id="audience-age-metrics">
                        <ApexChart TItem="AudienceAgeMetrics" Height=375 Options=AudienceAgeMetricsChartOptions>
                            <ApexPointSeries TItem="AudienceAgeMetrics"
                                Items="audienceAgeMetrics"
                                SeriesType="@SeriesType.Bar"
                                Name="Total Audience"
                                XValue="@(e => e.Week)"
                                YAggregate="@(e => e.Sum(e => e.Sessions))" />
                        </ApexChart>

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-12">
                <div class="card custom-card">
                    <div class="card-header justify-content-between">
                        <div class="card-title">
                            Suggestions
                        </div>
                        <a href="javascript:void(0);" class="text-muted">View All<i
                                class="ti ti-arrow-narrow-right ms-1"></i></a>
                    </div>
                    <div class="card-body">
                        <ul class="list-unstyled social-suggestions-list">                   
                            @foreach (var suggestion in suggestions)
                            {
                                <li>
                                    <div class="d-flex align-items-center gap-3">
                                        <div class="lh-1">
                                            <span class="avatar avatar-md">
                                                <img src="@suggestion.AvatarUrl" alt="@suggestion.Name" />
                                            </span>
                                        </div>
                                        <div class="flex-fill">
                                            <span class="fw-semibold d-block">@suggestion.Name</span>
                                            <span class="text-muted d-block">@suggestion.MutualFriends</span>
                                        </div>
                                        <div>
                                            <button aria-label="button" type="button" class="btn btn-sm btn-icon btn-light border">
                                                <i class="ri-user-add-line"></i>
                                            </button>
                                        </div>
                                    </div>
                                </li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-xl-12">
                <div class="card custom-card">
                    <div class="card-header justify-content-between">
                        <div class="card-title">Engagement</div>
                        <div class="d-flex align-items-center gap-2">
                            <span class="fs-12 text-success"><i class="ti ti-arrow-up"></i>2.45%</span>
                            <h5 class="fw-semibold mb-0">231,232</h5>
                        </div>
                    </div>
                    <div class="card-body p-0">
                        <div id="social-engagement">
                        <ApexChart TItem="EngagementData" Options="EngagementOptions" Height="195">
                            @foreach (var source in engagements.GroupBy(e => e.Source))
                            {
                                <ApexPointSeries TItem="EngagementData"
                                                Items="source.OrderBy(e => e.WeekNumber)"
                                                Name="@source.Key!.ToString()"
                                                SeriesType="SeriesType.Heatmap"
                                                XValue="@(e => e.WeekName)"
                                                YAggregate="@(e => (int)e.Average(a => a.Engagement))" />
                            }
                        </ApexChart>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End:: row-1 -->

<!-- Charts -->
@code{

   // ProfileVisits Report start //
    private List<ProfileVisits> profileVisits { get; set; } = ProfileVisitsData.GetProfileVisits();

    private ApexChartOptions<ProfileVisits> ProfileVisitsChartOptions { get; set; } = new ApexChartOptions<ProfileVisits>
    {
        Chart = new(){
            Height = 320,
            Type = ChartType.Line,
            Toolbar = new ApexCharts.Toolbar
            {
                Tools = new Tools
                {
                    Zoom = false,
                    Zoomin = false,
                    Zoomout = false,
                    Pan = false,
                    Reset = false,
                    Download = false
                },
            },
            DropShadow = new ApexCharts.DropShadow {
                    Enabled = true,
                    Top = 7,
                    Left = 1,
                    Blur = 3,
                    Color = "#000",
                    Opacity = 0.05
                },
        },
        Grid = new Grid
        {
            BorderColor = "#f5f4f4",
            StrokeDashArray = 5,
            Yaxis = new(){
                Lines = new(){
                    Show = true
                }
            }
        },
        Colors = new List<string> { "var(--primary-color)", "rgba(255, 73, 205, 1)", "rgba(50, 212, 132, 1)"},
        Stroke = new Stroke
        {
            Curve = [Curve.Smooth,Curve.Smooth,Curve.Smooth],
            Width = [2, 2, 2],
        },
        DataLabels = new DataLabels
        {
            Enabled = false,
        },
        
        Legend = new Legend
        {
            Show = true,
            Position = LegendPosition.Top,
            Markers = new(){
                Height = 5,
                Width = 5
            }
        },
        Xaxis =  new XAxis {
                Categories=new[] {"2019", "2020", "2021", "2023", "2024", "2025"},
                AxisBorder = new () {
                    Show = false,
                    Color = "rgba(119, 119, 142, 0.05)",
                    OffsetX = 0,
                    OffsetY = 0,
                },
                AxisTicks = new () {
                    Show = false,
                    Color = "rgba(119, 119, 142, 0.05)",
                    OffsetX = 0,
                    OffsetY = 0
                },
                Labels= new () {
                    Rotate = -90
                }
            },
          Yaxis = new List<YAxis>() {
                new YAxis
                {
                    Title= new() {
                        Style = new() {
                            Color = "#adb5be",
                            FontSize = "14px",
                            FontFamily = "Montserrat, sans-serif",
                            FontWeight = 600,
                            CssClass = "apexcharts-yaxis-label",
                        },
                    },
                    AxisBorder = new AxisBorder {
                        Show = true,
                        Color = "rgba(119, 119, 142, 0.05)",
                        OffsetX = 0,
                        OffsetY = 0
                    },
                     AxisTicks = new () {
                        Show = true,
                        BorderType = ApexCharts.BorderType.Solid,
                        Color = "rgba(119, 119, 142, 0.05)",
                        OffsetX = 0,
                        OffsetY = 0,
                    },
                }
            },
        Tooltip = new Tooltip
        {
            Theme = ApexCharts.Mode.Dark,
        }
    };

    public class ProfileVisits
    {
        public string? Month { get; set; }
        public decimal Facebook { get; set; }
        public decimal Instagram { get; set; }
        public decimal Twitter { get; set; }
    }

    public static class ProfileVisitsData
    {
        public static List<ProfileVisits> GetProfileVisits()
        {
            return new List<ProfileVisits>
            {
                new ProfileVisits { Month = "Jan", Facebook = 32, Instagram = 56, Twitter = 48  },
                new ProfileVisits { Month = "Feb", Facebook = 15, Instagram = 58, Twitter = 29 },
                new ProfileVisits { Month = "Mar", Facebook = 63, Instagram = 38, Twitter = 50 },
                new ProfileVisits { Month = "Apr", Facebook = 51, Instagram = 50, Twitter = 69 },
                new ProfileVisits { Month = "May", Facebook = 36, Instagram = 64, Twitter = 20 },
                new ProfileVisits { Month = "Jun", Facebook = 62, Instagram = 45, Twitter = 59 },
                new ProfileVisits { Month = "Jul", Facebook = 99, Instagram = 55, Twitter = 52 },
                new ProfileVisits { Month = "Aug", Facebook = 42, Instagram = 32, Twitter = 12 },
                new ProfileVisits { Month = "Sep", Facebook = 78, Instagram = 15, Twitter = 48 },
                new ProfileVisits { Month = "Aug", Facebook = 76, Instagram = 63, Twitter = 28 },
                new ProfileVisits { Month = "Nov", Facebook = 32, Instagram = 51, Twitter = 17 },
                new ProfileVisits { Month = "Dec", Facebook = 20, Instagram = 36, Twitter = 68 },
            };
        }
    }
    // ProfileVisits Report end //


    // Audience By Gender Start //
    private List<RadarMulti> RadarMultis { get; set; } = DataRadarMulti.RadarMulti();
    private ApexChartOptions<RadarMulti> RadarMultiChartOptions { get; set; } = new();
    public static class DataRadarMulti
    {
        public static List<RadarMulti> RadarMulti()
        {
            return new List<RadarMulti>
            {
                new RadarMulti { Country = "2019",  Male=20, Female=44},
