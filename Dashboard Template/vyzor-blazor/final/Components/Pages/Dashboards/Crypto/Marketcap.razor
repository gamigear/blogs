    private ApexChartOptions<T> InitializeGraphChartOptions<T>(List<string> colors) where T : class, IBarGraph
    {
        return new ApexChartOptions<T>
        {
            Chart = new()
            {
                Height = 30,
                Width = 120,
                Type = ChartType.Line,
                Toolbar = new ApexCharts.Toolbar
                {
                    Tools = new Tools { Zoom = false, Zoomin = false, Zoomout = false, Pan = false, Reset = false, Download = false },
                },
                Sparkline = new()
                {
                    Enabled = true,
                },
                DropShadow = new() {
                    Enabled = true,
                    Top = 0,
                    Left = 0,
                    Blur = 3,
                    Color = "#000",
                    Opacity = 0.1,
                },
            },
            Stroke = new Stroke
            {
                Show = true,
                Curve = [Curve.Smooth],
                LineCap = ApexCharts.LineCap.Butt,
                Width = 1,
                DashArray = 0
            },
            Yaxis = new List<YAxis>
            {
                new YAxis
                {
                    Min = 0,
                    Show = false,
                    AxisBorder = new AxisBorder { Show = false },
                }
            },
            Xaxis = new XAxis
            {
                AxisBorder = new AxisBorder { Show = false }
            },
            Tooltip = new Tooltip { 
                Enabled= false,
            },
            Colors = new Color(colors),
        };
    }

