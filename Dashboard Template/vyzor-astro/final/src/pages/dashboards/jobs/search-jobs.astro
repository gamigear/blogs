---
import Layout from "../../../layouts/Layout.astro";
import Pageheader from "../../../components/Pageheader.astro";
import { JobTable } from "../../../shared/data/dashboards/jobs/searchjobdata";
import "../../../scripts/libs/nouislider/nouislider.min.css";
---

<Layout Seo="Jobs-Search Jobs">
